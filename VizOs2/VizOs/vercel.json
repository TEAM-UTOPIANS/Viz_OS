{
  "version": 2,
  "builds": [
    {
      "src": "api/status.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/status",
      "dest": "api/status.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ],
  "functions": {
    "api/index.py": {
      "includeFiles": "backend/**"
    }
  }
}

